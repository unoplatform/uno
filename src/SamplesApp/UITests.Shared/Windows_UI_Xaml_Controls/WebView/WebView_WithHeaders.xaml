<UserControl
	x:Class="Uno.UI.Samples.Content.UITests.WebView.WebView_WithHeaders" 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:u="using:Uno.UI.Samples.Controls"
	xmlns:ios="http://uno.ui/ios"
	xmlns:win="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:android="http://uno.ui/android"
 	xmlns:not_win="http://uno.ui/not_win"
	mc:Ignorable="d ios android not_win"
	d:DesignHeight="2000"
	d:DesignWidth="400">

	<not_win:Grid HorizontalAlignment="Stretch"
		  VerticalAlignment="Stretch">
		<StackPanel Orientation="Vertical">
			<TextBlock Text="WebView_NavigateWithHttpRequestMessage" />
			<WebView Source="about:blank"
						 x:Name="MyWebView2"
						 Height="100"
						 Width="100" />
			<TextBlock x:Name="MyTextBlock" TextWrapping="Wrap" />
			<Button Content="Navigate to httpbin.org" x:Name="MyButton" />
		</StackPanel>
	</not_win:Grid>
</UserControl>
