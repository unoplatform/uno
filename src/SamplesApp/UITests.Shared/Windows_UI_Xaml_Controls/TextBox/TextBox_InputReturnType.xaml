<UserControl
	x:Class="Uno.UI.Samples.Content.UITests.TextBoxControl.TextBox_InputReturnType"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:not_win="http://uno.ui/not_win"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d not_win">

	<not_win:StackPanel>
		<TextBox x:Name="TestTextBox"
		 		 Header="Enter data:"
		         Text="" />
		<ComboBox x:Name="InputReturnTypesComboBox" ItemsSource="{x:Bind InputReturnTypes}" />
		<Button Click="SetHintClick">Set hint</Button>
	</not_win:StackPanel>

</UserControl>
