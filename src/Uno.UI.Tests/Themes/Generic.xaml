					xmlns:local="using:Uno.UI.Tests.App.Views">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="ms-appx:///Uno.UI.Tests/Themes/Controls/GenericXamlMergedControl.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<!-- Default style for GenericXamlTestControl - tests app-level Generic.xaml discovery -->
