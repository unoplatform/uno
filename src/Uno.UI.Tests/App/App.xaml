<Application x:Class="UnitTestsApp.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Xaml/Test_Dictionary.xaml" />
				<ResourceDictionary>
					<ResourceDictionary.MergedDictionaries>
						<ResourceDictionary>
							<SolidColorBrush x:Key="HangingOutInRecursiveMergedColorBrush"
											 Color="DarkMagenta" />
						</ResourceDictionary>
					</ResourceDictionary.MergedDictionaries>
					<SolidColorBrush x:Key="JustHangingOutInMergedDictColorBrush"
									 Color="Maroon" />
					<SolidColorBrush x:Key="HangingOutTooColorBrush"
									 Color="Magenta" />
				</ResourceDictionary>
			</ResourceDictionary.MergedDictionaries>
			<ResourceDictionary.ThemeDictionaries>
				<ResourceDictionary x:Key="Light">
					<SolidColorBrush x:Key="LucrativeColorBrush"
									 Color="Azure" />
				</ResourceDictionary>
			</ResourceDictionary.ThemeDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</Application>
