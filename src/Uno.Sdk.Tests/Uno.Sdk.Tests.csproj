<Project Sdk="MSTest.Sdk/3.10.0">

	<PropertyGroup>
		<TargetFramework>$(NetCurrent)</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IsPackable>false</IsPackable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="AwesomeAssertions" />
		<PackageReference Include="Microsoft.Build.Framework" Version="17.8.43" />
		<PackageReference Include="Microsoft.Build.Utilities.Core" Version="17.8.43" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Uno.Sdk\Uno.Sdk.csproj">
			<ReferenceOutputAssembly>true</ReferenceOutputAssembly>
			<OutputItemType>Analyzer</OutputItemType>
			<SetTargetFramework>TargetFramework=netstandard2.0</SetTargetFramework>
		</ProjectReference>
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Uno.Sdk_v0">
			<HintPath>..\Uno.Sdk\bin\$(Configuration)\netstandard2.0\Uno.Sdk_v0.dll</HintPath>
			<Private>true</Private>
		</Reference>
	</ItemGroup>

</Project>
