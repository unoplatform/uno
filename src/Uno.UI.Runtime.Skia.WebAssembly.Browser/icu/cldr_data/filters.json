{
    "strategy": "additive",
    "featureFilters": {
        "brkitr_rules": {
            "filterType": "regex",
            "excludelist": [
                "^.*loose.*$",
                "^.*char.*$",
                "^.*word.*$",
                "^.*sent.*$",
                "^.*title.*$"
            ]
        },
        "brkitr_dictionaries": {
            "excludelist": [
                "burmesedict",
                "khmerdict",
                "laodict"
            ]
        },
        "brkitr_tree": "include",
        "uemoji": "include",
        "cnvalias": "include",
        "locales_tree": "include"
    },
    "localeFilter": {
        "filterType": "locale",
        "includeScripts": false,
        "includeChildren": false,
        "whitelist": [
            "en_US"
        ]
    }
}